    <TextView
        android:id="@+id/tv_gas"
        android:layout_width="120dp"
        android:layout_height="match_parent"
        android:gravity="center"
        android:text="0"
        android:textColor="@color/color_333333"
        android:textSize="11sp" />

    <TextView
        android:id="@+id/tv_fil_t"
        android:layout_width="90dp"
        android:layout_height="match_parent"
        android:gravity="center"
        android:text="0"
        android:textColor="@color/color_333333"
        android:textSize="11sp" />

    <TextView
        android:id="@+id/tv_fil"
        android:layout_width="90dp"
        android:layout_height="match_parent"
        android:gravity="center"
        android:text="0"
        android:textColor="@color/color_333333"
        android:textSize="11sp" />

    <TextView
        android:id="@+id/tv_25"
        android:layout_width="90dp"
        android:layout_height="match_parent"
        android:gravity="center"
        android:text="0"
        android:textColor="@color/color_333333"
        android:textSize="11sp" />

    <TextView
        android:id="@+id/tv_75"
        android:layout_width="90dp"
        android:layout_height="match_parent"
        android:gravity="center"
        android:text="0"
        android:textColor="@color/color_333333"
        android:textSize="11sp" />


</LinearLayout>